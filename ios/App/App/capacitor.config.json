{
	"appId": "ee.forgr.capacitor_go",
	"appName": "Capgo",
	"webDir": "dist",
	"bundledWebRuntime": false,
	"plugins": {
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"SplashScreen": {
			"launchAutoHide": false,
			"androidScaleType": "CENTER_CROP"
		},
		"CapacitorUpdater": {
			"autoUpdateUrl": "https://capgo.app/api/latest?appid=ee.forgr.capacitor_go&channel=production"
		}
	}
}
